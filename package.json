{
  "name": "wodore",
  "version": "0.0.1",
  "description": "Where does your next tour peak?",
  "productName": "Wodore",
  "author": "Tobias Burgherr <tb@wodore.com>",
  "type": "module",
  "private": true,
  "scripts": {
    "lint": "eslint --ext .js,.ts,.vue ./",
    "lint:fix": "eslint --fix --ext .js,.ts,.vue ./",
    "format": "prettier --write \"**/*.{js,ts,vue,scss,html,md,json}\"",
    "test": "echo \"No test specified\" && exit 0",
    "dev": "yarn run dev:pwa",
    "dev:spa": "quasar dev -m spa --port 9000",
    "dev:ssr": "quasar dev -m ssr --port 9000",
    "dev:pwa": "quasar dev -m pwa --port 9000",
    "build": "yarn run build:pwa",
    "build:spa": "quasar build -m spa",
    "build:ssr": "quasar build -m ssr",
    "build:pwa": "quasar build -m pwa",
    "serve:spa": "quasar serve --history --port 9000 -H localhost --cors dist/spa/",
    "serve:ssr": "quasar serve --history --port 9000 -H localhost --cors dist/ssr/",
    "serve:pwa": "quasar serve --history --port 9000 -H localhost --cors dist/pwa/",
    "serve": "yarn run serve:pwa",
    "gen:api": "openapi-typescript https://api.wodore.com/v1/openapi.json -o ./src/clients/wodore_v1.d.ts",
    "preicons:fix": "npm exec -c 'rm -rf src/extras/icons/svg/build/fixed/*'",
    "icons:fix": "yarn run oslllo-svg-fixer -s src/extras/icons/svg/source/ -d src/extras/icons/svg/build/fixed --sp",
    "pregen:icons": "yarn run icons:fix",
    "gen:icons": "fantasticon src/extras/icons/svg/build/fixed -o src/extras/icons/dist -p wd --normalize -t woff woff2",
    "gen:icons:watch": "watch 'yarn run gen:icons' src/extras/icons/svg/source",
    "postgen:icons": "yarn run eslint --fix src/extras/icons/dist/icons.ts",
    "gen:favs": "icongenie generate -p src/assets/icongenie/",
    "gen:realfavs": "real-favicon generate src/assets/realfavgen.json src/assets/realfavgen_result.json public/realfavgen"
  },
  "dependencies": {
    "@fontsource/roboto-condensed": "^5.0",
    "@indoorequal/vue-maplibre-gl": "^7",
    "@quasar/extras": "^1.16",
    "@stripe/stripe-js": "^3.4.1",
    "@types/umami": "^2.10.0",
    "@vue-stripe/vue-stripe": "^4.5.0",
    "axios": "^1.2",
    "crypto-js": "^4.2.0",
    "maplibre-gl": "^4.1",
    "openapi-fetch": "^0.9",
    "openapi-typescript": "^6.7",
    "pinia": "^2.0",
    "quasar": "^2.16",
    "register-service-worker": "^1.0.0",
    "vue": "^3.4",
    "vue-i18n": "^9.2.2",
    "vue-router": "^4.0.12",
    "vue3-text-clamp": "^0.1.2",
    "watch": "^1.0.2"
  },
  "devDependencies": {
    "@iconify/json": "^2.2.202",
    "@intlify/unplugin-vue-i18n": "^2.0.0",
    "@quasar/app-vite": "^2.0.0-beta.12",
    "@quasar/icongenie": "^4.0.0",
    "@types/crypto-js": "^4.2.2",
    "@types/node": "^20.5.9",
    "@typescript-eslint/eslint-plugin": "^6.6.0",
    "@typescript-eslint/parser": "^6.6.0",
    "@vue/compiler-sfc": "^3.4.23",
    "autoprefixer": "^10.4.2",
    "cli-real-favicon": "^0.0.8",
    "dotenv": "^16.4.5",
    "eslint": "^8.11.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-vue": "^9.0.0",
    "fantasticon": "^3.0.0",
    "oslllo-svg-fixer": "^4.0.1",
    "prettier": "^3.0.3",
    "typescript": "~5.3.0",
    "unplugin-icons": "^0.18.5",
    "unplugin-vue-components": "^0.26.0",
    "vite-plugin-checker": "^0.6.4",
    "vite-tsconfig-paths": "^4.3.2",
    "vue-tsc": "^1.8.22",
    "workbox-build": "^7.0.0",
    "workbox-cacheable-response": "^7.0.0",
    "workbox-core": "^7.0.0",
    "workbox-expiration": "^7.0.0",
    "workbox-precaching": "^7.0.0",
    "workbox-routing": "^7.0.0",
    "workbox-strategies": "^7.0.0"
  },
  "engines": {
    "node": "^24 || ^22 || ^20 || ^18",
    "npm": ">= 6.13.4",
    "yarn": ">= 1.21.1"
  }
}
