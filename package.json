{
  "name": "wodore",
  "version": "0.0.6",
  "description": "Where does your next tour peak?",
  "productName": "Wodore",
  "author": "Tobias Burgherr <tb@wodore.com>",
  "type": "module",
  "private": true,
  "scripts": {
    "lint": "eslint --ext .js,.ts,.vue ./",
    "lint:fix": "eslint --fix --ext .js,.ts,.vue ./",
    "format": "prettier --write \"**/*.{js,ts,vue,scss,html,md,json}\"",
    "test": "echo \"No test specified\" && exit 0",
    "dev": "yarn run dev:pwa",
    "dev:spa": "quasar dev -m spa --port 9000",
    "dev:ssr": "quasar dev -m ssr --port 9000",
    "dev:pwa": "quasar dev -m pwa --port 9000",
    "build": "yarn run build:pwa",
    "build:spa": "quasar build -m spa",
    "build:ssr": "quasar build -m ssr",
    "build:pwa": "quasar build -m pwa",
    "docker:build": "yarn docker:pwa",
    "docker:build-plain": "docker build --progress plain -t wodore-frontend --build-arg GIT_HASH=$(git rev-parse --short HEAD) .",
    "docker:pwa": "docker build  -t wodore-frontend --build-arg GIT_HASH=$(git rev-parse --short HEAD) .",
    "docker:ls": "docker image ls wodore-frontend",
    "docker:login": "echo \"Run in a terminal:\n\necho \\$DOCKER_GITHUB_TOKEN | docker login ghcr.io -u wodore -p --password-stdin\"",
    "docker:run-env": "docker run -p 9001:8080  --env-file .env wodore-frontend",
    "docker:run-dev": "sh -c 'cat .env .env.local .env.dev .env.local.dev 2>/dev/null > /tmp/merged-local.env || true && docker run -p 9002:8080 --env-file /tmp/merged-local.env wodore-frontend'",
    "docker:run-prod": "sh -c 'cat .env .env.local .env.prod .env.local.prod 2>/dev/null > /tmp/merged-prod.env || true && docker run -p 9003:8080 --env-file /tmp/merged-prod.env wodore-frontend'",
    "docker:publish": "scripts/check-git-status.sh && yarn docker:build && docker image tag wodore-frontend ghcr.io/wodore/wodore-frontend-quasar:edge-pwa && docker push ghcr.io/wodore/wodore-frontend-quasar:edge-pwa",
    "release": "scripts/release.sh",
    "serve:spa": "quasar serve --history --port 9000 -H localhost --cors dist/spa/",
    "serve:ssr": "quasar serve --history --port 9000 -H localhost --cors dist/ssr/",
    "serve:pwa": "quasar serve --history --port 9000 -H localhost --cors dist/pwa/",
    "serve": "yarn run serve:pwa",
    "gen:api": "openapi-typescript https://api.wodore.com/v1/openapi.json -o ./src/clients/wodore_v1.d.ts; yarn run format",
    "gen:api-local": "openapi-typescript http://127.0.0.1:8000/v1/openapi.json -o ./src/clients/wodore_v1.d.ts; yarn run format",
    "preicons:fix": "npm exec -c 'rm -rf src/extras/icons/svg/build/fixed/*'",
    "icons:fix": "yarn run oslllo-svg-fixer -s src/extras/icons/svg/source/ -d src/extras/icons/svg/build/fixed --sp",
    "pregen:icons": "yarn run icons:fix",
    "gen:icons": "fantasticon src/extras/icons/svg/build/fixed -o src/extras/icons/dist -p wd --normalize -t woff woff2",
    "gen:icons:watch": "watch 'yarn run gen:icons' src/extras/icons/svg/source",
    "postgen:icons": "yarn run eslint --fix src/extras/icons/dist/icons.ts",
    "gen:favs": "icongenie generate -p src/assets/icongenie/",
    "gen:realfavs": "real-favicon generate src/assets/realfavgen.json src/assets/realfavgen_result.json public/realfavgen"
  },
  "dependencies": {
    "@fontsource/roboto-condensed": "^5.0",
    "@indoorequal/vue-maplibre-gl": "^8",
    "@mapbox/mapbox-gl-draw": "^1.4.3",
    "@mapbox/polyline": "^1.2.1",
    "@pmndrs/detect-gpu": "^6.0.0",
    "@quasar/extras": "^1.16.12",
    "@stripe/stripe-js": "^8.6.4",
    "@types/mapbox__mapbox-gl-draw": "^1.4.7",
    "@types/umami": "^2.10.0",
    "@vue-stripe/vue-stripe": "^5.3.0",
    "@vueuse/core": "^14",
    "axios": "^1.2",
    "crypto-js": "^4.2.0",
    "js-sha256": "^0.11.0",
    "mapbox-gl-draw-waypoint": "^1.2.3",
    "maplibre-gl": "^5",
    "oidc-client-ts": "^3.0.1",
    "openapi-fetch": "^0.15.0",
    "openapi-typescript": "^7.10.1",
    "openmeteo": "^1.2.3",
    "pinia": "^3.0.4",
    "quasar": "^2.17.0",
    "register-service-worker": "^1.0.0",
    "vue": "^3",
    "vue-i18n": "^11",
    "vue-router": "^4",
    "vue3-text-clamp": "^0.1.2",
    "watch": "^1.0.2"
  },
  "devDependencies": {
    "@iconify/json": "^2.2.202",
    "@intlify/unplugin-vue-i18n": "^11",
    "@quasar/app-vite": "^2",
    "@quasar/icongenie": "^4.0.0",
    "@types/crypto-js": "^4.2.2",
    "@types/mapbox__polyline": "^1.0.5",
    "@types/node": "^25.0.10",
    "@typescript-eslint/eslint-plugin": "^8.53.1",
    "@typescript-eslint/parser": "^8.53.1",
    "@vue/compiler-sfc": "^3.4.23",
    "autoprefixer": "^10.4.2",
    "cli-real-favicon": "^0.0.9",
    "dotenv": "^17.2.3",
    "eslint": "^9.39.2",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-vue": "^10.7.0",
    "fantasticon": "^4.1.0",
    "git-cliff": "^2.8.0",
    "git-rev-sync": "^3.0.2",
    "lint-staged": "^16.2.7",
    "oslllo-svg-fixer": "^6.0.1",
    "prettier": "^3.0.3",
    "typescript": "~5.9.3",
    "typescript-eslint": "^8.53.1",
    "unplugin-icons": "^23.0.1",
    "unplugin-vue-components": "^31.0.0",
    "vite-plugin-checker": "^0.12.0",
    "vite-tsconfig-paths": "^6.0.5",
    "vue-eslint-parser": "^10.2.0",
    "vue-tsc": "^3.2.3",
    "workbox-build": "^7.0.0",
    "workbox-cacheable-response": "^7.0.0",
    "workbox-core": "^7.0.0",
    "workbox-expiration": "^7.0.0",
    "workbox-precaching": "^7.0.0",
    "workbox-routing": "^7.0.0",
    "workbox-strategies": "^7.0.0"
  },
  "engines": {
    "node": "^24 || ^22",
    "npm": ">= 6.13.4",
    "yarn": ">= 1.21.1"
  },
  "packageManager": "yarn@1.22.22+sha1.ac34549e6aa8e7ead463a7407e1c7390f61a6610"
}
